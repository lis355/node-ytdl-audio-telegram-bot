	"version": "0.2.0",
	"configurations": [
		{
			"name": "start",
			"type": "node",
			"runtimeExecutable": "${env:APPDATA}/nvm/v24.1.0/node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/start.js",
			"console": "integratedTerminal",
			"outputCapture": "std",
			"cwd": "${workspaceFolder}",
			"env": {
				"DEVELOPER_ENVIRONMENT": "true"
			},
			"args": []
		},
		{
			"name": "startLocalDownloader",
			"type": "node",
			"runtimeExecutable": "${env:APPDATA}/nvm/v24.1.0/node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/startLocalDownloader.js",
			"console": "integratedTerminal",
			"outputCapture": "std",
			"cwd": "${workspaceFolder}",
			"env": {
				"DEVELOPER_ENVIRONMENT": "true"
			},
			"args": [
				"https://www.youtube.com/watch?v=BfFPlkPYZFo"
			]
		},
		{
			"name": "service-windows-install",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/service/windows/serviceInstall.js",
			"cwd": "${workspaceFolder}/service/windows/",
			"console": "integratedTerminal",
			"outputCapture": "std"
		},
		{
			"name": "service-windows-uninstall",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/service/windows/serviceUninstall.js",
			"cwd": "${workspaceFolder}/service/windows/",
			"console": "integratedTerminal",
			"outputCapture": "std"
		}
	]
}