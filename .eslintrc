{
	"extends": "protop",
	"globals": {
		"ndapp": "writable",
		"app": "writable"
	},
	"env": {
		"es2021": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "babel-eslint",
		"allowImportExportEverywhere": true
	},
	"rules": {
		"semi": [
			2,
			"always"
		]
	}
}